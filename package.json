{
    "name": "coverzy",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "env-cmd -f .env react-scripts start",
        "start:staging": "env-cmd -f .env.staging react-scripts start",
        "start:production": "env-cmd -f .env.production react-scripts start",
        "build": "env-cmd -f .env react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "build:production": "env-cmd -f .env.production react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "npx prettier --write 'src/**/*.{ts,tsx}'",
        "pm2-be-start-merge": "rm -rf build/ && sleep 2 && npm i && sleep 2 && npm run build && sleep 2 && pm2 serve build/ 3002 --name start-dev.covrzy.com --spa --watch",
        "pm2-be-beta-merge": "rm -rf build/ && sleep 2 && npm i && sleep 2 && npm run build && sleep 2 && pm2 serve build/ 3001 --name beta-dev.covrzy.com --spa --watch ",
        "pm2-be-build-start-beta-merge": "rm -rf build/ && sleep 2 && npm i && sleep 2 && npm run build && sleep 2 && pm2 serve build/ 3002 --name start-dev.covrzy.com --spa --watch && sleep 2  && pm2 serve build/ 3001 --name beta-dev.covrzy.com --spa --watch ",
        "pm2:staging": "pm2 serve /home/ubuntu/deploy-build/Frontend-Dev/build/ 3000 --name frontend-staging --spa --watch",
        "pm2:production": "pm2 serve /home/ubuntu/deploy-build/Frontend/build/ 4000 --name covrzy-frontend --spa --watch",
        "sync-prod": "yarn build:production && cp -r /home/ubuntu/Frontend/build /home/ubuntu/deploy-build/Frontend/",
        "sync-blogs": "yarn build:production && aws s3 sync build/ s3://covrzy.com && aws cloudfront create-invalidation --distribution-id E13W0CCCUI3MEJ --paths /*"
    },
    "dependencies": {
